- if user_signed_in?
  - if video.like_user(current_user.id)
    = button_to video_like_path(likes, video_id: video.id), method: :delete, id: "like-button", remote: true do
      = image_tag("icon_red_heart.png")
  - else
    = button_to video_likes_path(video), id: "like-button", remote: true do
      = image_tag("icon_heart.png")
