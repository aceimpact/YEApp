- if user_signed_in? && video.like_user(current_user.id)
  = link_to video_like_path(likes, video_id: video.id), method: :delete, id: "like-button", remote: true do
    = image_tag("icon_red_heart.png")
- else
  = link_to video_likes_path(video), method: :post, id: "like-button", remote: true do
    = image_tag("icon_heart.png")
