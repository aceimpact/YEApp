<% provide(:title, 'HOME') %>

<div class="main">
<section class="home-section bg-dark-60 portfolio-page-header parallax-bg" id="home" data-background="./images/portfolio_header_bg.jpg">
  <div class="titan-caption">
    <div class="caption-content">
      <div class="font-alt mb-30 titan-title-size-1">Hello &amp; welcome</div>
      <div class="font-alt mb-40 titan-title-size-4">YEApp</div>
    </div>
  </div>
</section>

<%= @video.each do |video| %>
<section class = "card">
  <div class = "col-lg-4">
    <h4><%= video.name %></h4>
    <iframe id="player" type="text/html" src="https://www.youtube.com/watch?v#{video.video_id}"></iframe>
    <p><%= video.comment %></p>
    <%= link_to "video.users.name" %><br />
  </div>
<% end %>
</div>
