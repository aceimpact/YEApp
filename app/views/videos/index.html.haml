- provide(:title, 'HOME')
%section#home.home-section.bg-dark-60.portfolio-page-header.parallax-bg
  .titan-caption
    .caption-content
      .font-alt.mb-30.titan-title-size-1 Hello &amp; welcome
      .font-alt.mb-40.titan-title-size-4 YEApp
.index-content
  .container
    - @videos.each do |video|
      .col-lg-4
        .card
          %h4= video.name
          %iframe#player{:frameborder => "0", :height => "200", :src => "https://www.youtube.com/embed/\#{video.uuid}", :type => "text/html", :width => "320"}
          %p= video.comment
          = link_to "#{video.user.name}"
          %br/
          - if user_signed_in?
            - if Like.find_by(user_id: current_user.id, video_id: @video.id)
              = link_to video_like_path(video, like), method: :delete do
                %span.fa.fa-heart.like-btn-unlike
            - else
              = link_to video_likes_path(video) do
                %span.fa.fa-heart.like-btn
